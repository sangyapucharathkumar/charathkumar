import java.util.Scanner;
class for_loop {
    public static void main(String args[]){
        try (Scanner sc = new Scanner(System.in)) {
            int n;
            System.out.print("enter number:");
            n=sc.nextInt();
            System.out.println("1 to "+n+" numbers are:");
            for(int i=1;i<=n;i++){
                System.out.println("\t"+i);
            }
        }
    }
}
 
